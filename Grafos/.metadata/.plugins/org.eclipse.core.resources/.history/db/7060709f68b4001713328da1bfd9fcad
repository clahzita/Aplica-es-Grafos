package br.imd.grafos.elementos;

import java.util.List;

/**
 * Representação dos nodos/vértices de um grafol
 * @version 1.0
 * @author clarissa
 *
 */
public class Vertice {
	private int grau;
	private int cor;
	private List<Aresta> adj;
	
	public int getGrau() {
		return grau;
	}
	
	public void setGrau(int grau) {
		this.grau = grau;
	}

	public int getCor() {
		return cor;
	}

	public void setCor(int cor) {
		this.cor = cor;
	}
	
}
