 */
package br.imd.grafos.elementos;

/**
 * Representação de aresta do vértice
 * @version 1.0
 * @author clarissa
 *
 */
public class Aresta {
	private Vertice origem;
	private Vertice destino;
	private int peso;
	
	public Aresta(Vertice origem, Vertice destino, int peso) {
		super();
		this.origem = origem;
		this.destino = destino;
		this.peso = peso;
	}
	
	
	
	
	
}
